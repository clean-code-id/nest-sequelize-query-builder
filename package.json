{
  "name": "@cleancode-id/nestjs-sequelize-query-builder-workspace",
  "version": "2.0.4",
  "private": true,
  "workspaces": [
    "packages/core",
    "packages/example"
  ],
  "scripts": {
    "build": "npm run build --workspace=packages/core",
    "example:dev": "npm run start:dev --workspace=packages/example",
    "example:start": "npm run start --workspace=packages/example",
    "test": "npm run test --workspace=packages/core",
    "publish:dry": "npm run publish:dry --workspace=packages/core",
    "publish:prod": "npm run build && npm run publish:prod --workspace=packages/core",
    "deploy": "bash scripts/publish.sh"
  }
}
